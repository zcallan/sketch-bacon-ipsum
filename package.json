{
  "name": "sketch-bacon-ipsum",
  "version": "0.3.1",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "sketch-bacon-ipsum",
    "manifest": "src/manifest.json",
    "main": "plugin.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.4.0",
    "babel-cli": "^6.26.0",
    "babel-preset-es2017": "^6.24.1"
  },
  "author": "zcallan\n <kaln.delbridge@hotmail.com\n>",
  "repository": "https://github.com/zcallan/sketch-bacon-ipsum",
  "description": "A simple & easy Bacon Ipsum text generator for Sketch."
}
